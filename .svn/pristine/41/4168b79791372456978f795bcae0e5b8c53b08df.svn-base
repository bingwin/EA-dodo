        ];
        $inserOrder[] = array(
            'order' => $order_data,
            'orderDetail' => $orderDetail
        );
        return $inserOrder;
    }
    /**
     * 对象转为数组
     * @param object $array
     * @return array
     * @author czq
     */
    function object_array($array) {
        if (is_object($array)) {
            $array = (array) $array;
        }
        if (is_array($array)) {
            foreach ($array as $key => $value) {
                $array[$key] = $this->object_array($value);
            }
        }
        return $array;
    }

