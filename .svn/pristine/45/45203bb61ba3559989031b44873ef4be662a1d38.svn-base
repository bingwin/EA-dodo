namespace SeasLog;


class SeasLog extends \SeasLog
{
    public static function log($level, $message = NULL, $content = [], $logger = NULL)
    {
        try{
            parent::log($level, $message, $content, $logger);
        }catch (\Error $error){
            echo $error->getMessage();
        }

    }
}